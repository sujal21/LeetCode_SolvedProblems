class Solution {
    public int findContentChildren(int[] g, int[] s) {
        Arrays.sort(g);
        Arrays.sort(s);
        int count=0;
        int child=0;
        int cookie=0;
        while(child<g.length && cookie<s.length){
            if(s[cookie]>=g[child]){
                count++;
                child++;
            }
            cookie++;
        }
        return count;
    }
}
